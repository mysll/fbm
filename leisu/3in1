        String.prototype.showGoal2Number = function(){
            var numPattern = /^-?\d*\.?\d+$/;//数字正则
            var goal=this;
            if (!goal){
                return ""
            }else {
                if(goal.trim().match(numPattern)){
                    let len = goal.length;
                    let str='';
                    for(let i =0;i<len;i++){
                        str+= goal[i]
                    }
                    return str;
                }else{
                    return ""
                }
            }
        }
